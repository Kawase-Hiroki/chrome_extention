{
    "manifest_version": 3,
    "name": "Tab Info Manager",
    "version": "1.0",
    "description": "タブの情報を保存・復元・削除できる拡張機能",
    "permissions": [
      "tabs",
      "storage",
      "webNavigation"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  